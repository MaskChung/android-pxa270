<resources>
    <string-array name="pref_entries_message_ring_tone">
      <item>Robo Dream</item>
      <item>Robo Nightmare</item>
      <item>Electric Sheep</item>
    </string-array>

    <string-array name="pref_entry_values_message_ring_tone">
      <item>robo_dream</item>
      <item>robo_nightmare</item>
      <item>electric_sheep</item>
    </string-array>

    <string-array name="pref_entries_mms_creation_mode">
      <item>Restricted</item>
      <item>Warning</item>
      <item>Free</item>
    </string-array>

    <!-- The default for this array is
         "@string/pref_default_mms_creation_mode". -->
    <string-array name="pref_entry_values_mms_creation_mode">
      <item>restricted</item>
      <item>warning</item>
      <item>free</item>
    </string-array>

    <string-array name="pref_entries_mms_expiry">
      <item>one hour</item>
      <item>three hours</item>
      <item>one day</item>
      <item>one week</item>
    </string-array>

    <!-- These values are expressed in seconds. -->
    <string-array name="pref_entry_values_mms_expiry">
      <item>3600</item>
      <item>10800</item>
      <item>86400</item>
      <item>604800</item>
    </string-array>

    <string-array name="pref_entries_mms_max_size">
      <item>100K</item>
      <item>200K</item>
      <item>500K</item>
    </string-array>

    <!-- These values are expressed in bytes.  The default for this
         array is "@string/pref_default_mms_max_size". -->
    <string-array name="pref_entry_values_mms_max_size">
      <item>100000L</item>
      <item>200000L</item>
      <item>500000L</item>
    </string-array>

    <string-array name="pref_entries_mms_priority">
      <item>low</item>
      <item>medium</item>
      <item>high</item>
    </string-array>

    <string-array name="pref_entry_values_mms_priority">
      <item>low</item>
      <item>medium</item>
      <item>high</item>
    </string-array>

    <string-array name="pref_entries_mms_resubmission_mode">
      <item>Restricted</item>
      <item>Warning</item>
      <item>Free</item>
    </string-array>

    <!-- The default for this array is
         "@string/pref_default_mms_resubmission_mode". -->
    <string-array name="pref_entry_values_mms_resubmission_mode">
      <item>restricted</item>
      <item>warning</item>
      <item>free</item>
    </string-array>
</resources>
